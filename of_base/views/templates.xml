<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_backend" name="OF Base assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/of_base/static/src/js/of_form.js"></script>
            <script type="text/javascript" src="/of_base/static/src/js/of_action_manager.js"></script>
            <script type="text/javascript" src="/of_base/static/src/js/of_user_menu.js"/>
            <link rel="stylesheet" type="text/less" href="/of_base/static/src/less/of_base.less"/>
        </xpath>
   </template>

<data noupdate="1">
    <template id="layout_noindex" inherit_id="web.layout" name="OF Web Layout noindex" active="False">
        <xpath expr="//head" position="inside">
            <meta name="robots" content="noindex, nofollow"/>
        </xpath>
    </template>
</data>
</odoo>
