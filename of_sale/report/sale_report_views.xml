<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="of_sale_sale_report_search_view" model="ir.ui.view">
        <field name="name">of.sale.sale.report.search.view</field>
        <field name="model">sale.report</field>
        <field name="inherit_id" ref="sale.view_order_product_search"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='date']" position="after">
                <field name="of_partner_tags"/>
                <field name="of_product_tags"/>
            </xpath>
        </field>
    </record>

</odoo>
