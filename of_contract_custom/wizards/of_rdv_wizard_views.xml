<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="of_contract_view_rdv_intervention_wizard" model="ir.ui.view">
        <field name="name">of.contract.view.rdv.intervention.wizard</field>
        <field name="model">of.tournee.rdv</field>
        <field name="inherit_id" ref="of_planning_tournee.view_rdv_intervention_wizard"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='creer_recurrence']" position="replace"/>
            <xpath expr="//field[@name='service_id']" position="attributes">
                <attribute name="string">Demande d'intervention</attribute>
            </xpath>
        </field>
    </record>

</odoo>
