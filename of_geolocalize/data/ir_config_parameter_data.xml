<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">

    <record id="nominatim_base_url" model="ir.config_parameter">
        <field name="key">Nominatim_Base_URL</field>
        <field name="value">https://nominatim.openstreetmap.org/search</field>
    </record>
    
    <record id="deactivate_geocoding_on_create" model="ir.config_parameter">
    	<field name="key">Deactivate_Geocoding_On_Create</field>
    	<field name="value">0</field>
    </record>

    <record id="deactivate_geocoding_on_write" model="ir.config_parameter">
        <field name="key">Deactivate_Geocoding_On_Write</field>
        <field name="value">0</field>
    </record>

</odoo>