<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_rdv_intervention_wizard_sav" model="ir.ui.view">
        <field name="name">Planifier une intervention (SAV)</field>
        <field name="model">of.tournee.rdv</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="of_planning_tournee.view_rdv_intervention_wizard"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='service_id']" position="before">
                <field name="sav_id" widget="selection"/>
            </xpath>
        </field>
    </record>

</odoo>