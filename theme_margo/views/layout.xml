<?xml version="1.0" encoding="utf-8" ?>
<openerp>
	<data>

		<template id="layout" inherit_id="website.layout">  
			<xpath expr="//html/body" position="inside">
			 		 <script type="text/javascript" src="/theme_margo/static/src/js/jquery.migrate.js"></script>
				    <script type="text/javascript" src="/theme_margo/static/src/js/modernizrr.js"></script>
				    <script type="text/javascript" src="/theme_margo/static/src/js/jquery.fitvids.js"></script>
				    <script type="text/javascript" src="/theme_margo/static/src/js/owl.carousel.min.js"></script>
				    <script type="text/javascript" src="/theme_margo/static/src/js/nivo-lightbox.min.js"></script>
				    <script type="text/javascript" src="/theme_margo/static/src/js/jquery.isotope.min.js"></script>
				    <script type="text/javascript" src="/theme_margo/static/src/js/jquery.appear.js"></script>	
				    <script type="text/javascript" src="/theme_margo/static/src/js/count-to.js"></script>
				    <script type="text/javascript" src="/theme_margo/static/src/js/jquery.textillate.js"></script>
				    <script type="text/javascript" src="/theme_margo/static/src/js/jquery.lettering.js"></script>
				    <script type="text/javascript" src="/theme_margo/static/src/js/jquery.easypiechart.min.js"></script>
				    <script type="text/javascript" src="/theme_margo/static/src/js/jquery.nicescroll.min.js"></script>
				    <script type="text/javascript" src="/theme_margo/static/src/js/jquery.parallax.js"></script>
				    <script type="text/javascript" src="/theme_margo/static/src/js/mediaelement-and-player.js"></script>
				    <script type="text/javascript" src="/theme_margo/static/src/js/script.js"></script> 
		    		<script type="text/javascript"  src="/theme_margo/static/src/js/custom.js"></script>  
		    
		    		
		    		
			</xpath>
			 		
			<xpath expr="//html/head" position="inside">
					<link href="/theme_margo/static/src/css/responsive.css" rel="stylesheet" type="text/css" /> 
				    <link href="/theme_margo/static/src/css/animate.css" rel="stylesheet" type="text/css" /> 
				    <link href="/theme_margo/static/src/css/style.css" rel="stylesheet" type="text/css" />
		   			<link href="/theme_margo/static/src/css/custom.css" rel="stylesheet" type="text/css" />
				</xpath>
				
		</template>   
		
		
		<!-- rewrite  is best to inherit -->
		<template id="website.submenu" name="Submenu Changed Style">
			    <li t-if="not submenu.child_id" >
			        <a t-att-href="(website.menu_id.child_id[0] == submenu) and '/' or submenu.url" t-ignore="true" t-att-target="'_blank' if submenu.new_window else None"
			        t-att-class="
			        ((submenu.url and submenu.url != '/' and request.httprequest.path.startswith(submenu.url)) or
			         request.httprequest.path == submenu.url) and 'active'
			        " >
			            <span t-field="submenu.name"/>
			        </a>
			    </li>
			    <li t-if="submenu.child_id" >
			        <a data-toggle="dropdown" href="#" t-attf-class="#{
			        ((submenu.url and submenu.url != '/' and [1 for submenu in submenu.child_id if request.httprequest.path.startswith(submenu.url)]) or
			         request.httprequest.path == submenu.url) and 'active'
			        }">
			            <span t-field="submenu.name"/> <span class="caret" t-ignore="true"></span>
			        </a>
			        <ul class="dropdown" role="menu">
			            <t t-foreach="submenu.child_id" t-as="submenu">
			                <t t-call="website.submenu"/>
			            </t>
			        </ul>
			    </li>
		</template>
		 
		<template id="top_header" name="Top Header" inherit_id="website.layout">
			<xpath expr="//div[@id='wrapwrap']/header/div" position="before">
		            <!-- Start Top Bar -->
		            <div class="top-bar">
		                <div class="container">
		                
		                    <div class="row">
		                        <div class="col-md-7">
		                            <!-- Start Contact Info -->
		                            <ul class="contact-details">
		                                <li><a href="#"><i class="fa fa-map-marker"></i> <span t-field="res_company.street"/><span t-field="res_company.street2"/> ,<span t-field="res_company.country_id"/></a>
		                                </li>
		                                <li><a href="#"><i class="fa fa-envelope-o"></i> <span t-field="res_company.email"></span> </a>
		                                </li>
		                                <li><a href="#"><i class="fa fa-phone"></i> <span t-field="res_company.phone"></span></a>
		                                </li>
		                            </ul>
		                            <!-- End Contact Info -->
		                        </div><!-- .col-md-6 -->
		                        <div class="col-md-5">
		                            <!-- Start Social Links -->
		                            <ul class="social-list">
		                                <li>
		                                    <a class="facebook itl-tooltip" data-placement="bottom" title="Facebook" href="#"><i class="fa fa-facebook"></i></a>
		                                </li>
		                                <li>
		                                    <a class="twitter itl-tooltip" data-placement="bottom" title="Twitter" href="#"><i class="fa fa-twitter"></i></a>
		                                </li>
		                                <li>
		                                    <a class="google itl-tooltip" data-placement="bottom" title="Google Plus" href="#"><i class="fa fa-google-plus"></i></a>
		                                </li>
		                                <li>
		                                    <a class="linkdin itl-tooltip" data-placement="bottom" title="Linkedin" href="#"><i class="fa fa-linkedin"></i></a>
		                                </li>
		                                <li>
		                                    <a class="skype itl-tooltip" data-placement="bottom" title="Skype" href="#"><i class="fa fa-skype"></i></a>
		                                </li>
		                            </ul>
		                            <!-- End Social Links -->
		                        </div><!-- .col-md-6 -->
		                    </div><!-- .row -->
		                </div><!-- .container -->
		            </div><!-- .top-bar -->
		            <!-- End Top Bar -->
			</xpath>
		
			<xpath expr="//header//div[@class='navbar navbar-default navbar-static-top']" position="attributes">
		           <attribute name='class'>navbar navbar-default navbar-top</attribute>                    
		    </xpath>
		          
			<xpath expr="//header//div[@class='navbar navbar-default navbar-top']//div[@class='container']//div[@class='navbar-header']//button" position="replace">
		    	<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
		        	<i class="fa fa-bars"></i>
		        </button>
			</xpath>
		            
			<xpath expr="//header//div[@class='navbar navbar-default navbar-top']//div[@class='container']//div[@class='collapse navbar-collapse navbar-top-collapse']" position="attributes">
		    	<attribute name='class'>navbar-collapse collapse</attribute>
			</xpath>
		         
			<xpath expr="//header//ul[@id='top_menu']" position="attributes">
		    	<attribute name='class'>nav navbar-nav navbar-right</attribute>                    
			</xpath>	 
		         
			<xpath expr="//header//ul[@id='top_menu']//li[@class='dropdown']" position="attributes">
		    	<attribute name='class'></attribute>                    
			</xpath>	
			
			<xpath expr="//header//ul[@id='top_menu']//ul" position="attributes">
		    	<attribute name='class'>dropdown</attribute>                    
			</xpath>
		        
			<xpath expr="//header//ul[@id='top_menu']" position="before">
		     	<div class="search-side">
		        	<a href="#" class="show-search"><i class="fa fa-search"></i></a>
		            	<div class="search-form">
		                	<form autocomplete="off" role="search" method="get" class="searchform" action="#">
		                    	<input type="text" value="" name="s" id="s" placeholder="Search the site..."/>
		                    </form>
		                </div>
		        </div>
			</xpath> 
			
			<xpath expr="//footer" position="replace">
			<!-- Start Footer Section -->
        <footer>
            <div class="container hidden-print">
                <div class="row footer-widgets">
                    
                    <div class="col-md-3 col-xs-12">
                        <div class="footer-widget mail-subscribe-widget">
                            <h4>Get in touch<span class="head-line"></span></h4>
                            <p>Join our mailing list to stay up to date and get notices about our new releases!</p>
                            <form class="subscribe">
                                <input type="text" placeholder="mail@example.com"/>
                                <input type="submit" class="btn-system" value="Send"/>
                            </form>
                        </div>
                        <div class="footer-widget social-widget">
                            <h4>Follow Us<span class="head-line"></span></h4>
                            <ul class="social-icons">
                                <li>
                                    <a class="facebook" href="#"><i class="fa fa-facebook"></i></a>
                                </li>
                                <li>
                                    <a class="twitter" href="#"><i class="fa fa-twitter"></i></a>
                                </li>
                                <li>
                                    <a class="google" href="#"><i class="fa fa-google-plus"></i></a>
                                </li>
                                <li>
                                    <a class="linkdin" href="#"><i class="fa fa-linkedin"></i></a>
                                </li>
                                <li>
                                    <a class="skype" href="#"><i class="fa fa-skype"></i></a>
                                </li>
                            </ul>
                        </div>
                    </div> 
                    
                     <div class="col-md-3 col-xs-12">
                        <div class="footer-widget">
                            <h4>Our products &amp; Services<span class="head-line"></span></h4>
                    		<ul class="list-unstyled" id="products">
                        <li><a href="/">Home</a></li>
                    </ul>
                        </div>
                    </div> 
                    
                    <div class="col-md-3 col-xs-12" id="info">
                        <div class="footer-widget">
                            <h4>Connect with us<span class="head-line"></span></h4>
                    		<ul class="list-unstyled">
                        		<li><a href="/page/website.contactus">Contact us</a></li>
                    		</ul>
                       <ul class="list-unstyled">
                        <li t-ignore="true"><i class="fa fa-phone"></i> <span t-field="res_company.phone"></span></li>
                        <li t-ignore="true"><i class="fa fa-envelope"></i>  <span t-field="res_company.email"></span></li>
                    </ul>
                        </div>
                    </div> 
                    
                 <div class="col-md-3 col-xs-12">
                    <div class="footer-widget contact-widget">
                        <h4>
                            <span t-field="res_company.name" />
                            <small> - <a href="/page/website.aboutus">About us</a></small>
                        </h4>
                        <p>
                            We are a team of passionate people whose goal is to improve everyone's
                            life through disruptive products. We build great products to solve your
                            business problems.
                        </p>
                        <p>
                            Our products are designed for small to medium size companies willing to optimize
                            their performance.
                        </p>
                    </div>
		                            
                     <ul class="list-inline js_language_selector mt16" t-if="(request.website_multilang and len(languages) &gt; 1) or editable">
                        <li t-foreach="languages" t-as="lg">
                            <a
                                t-att-href="url_for(request.httprequest.path + '?' + keep_query(), lang=lg[0])"
                                t-att-data-default-lang="editable and 'true' if lg[0] == website.default_lang_code else None"
                                t-att-data-lang="lg[0]"
                                class="js_change_lang"
                            >
                              <t t-esc="lg[1].split('/').pop()"/>
                            </a>
                        </li>
                        <li groups="base.group_website_publisher">
                            <t t-set="url_return" t-value="url_for('', '[lang]') + '?' + keep_query()"/>
                            <a t-attf-href="/web#action=base.action_view_base_language_install&amp;website_id=#{website.id}&amp;url_return=#{url_return}">
                            <i class="fa fa-plus-circle"/>
                            Add a language...
                            </a>
                        </li>
                    </ul>
                    
                        
                </div>
                
                </div> 

                <!-- Start Copyright -->
                <div class="copyright-section">
                    <div class="row">
                        <div class="col-md-6">
                            <p> Copyright &amp;copy; <span t-field="res_company.name" itemprop="name">Company name</span>-  All Rights Reserved  </p>
                        </div><!-- .col-md-6 -->
                        <div class="col-md-6">
                            <ul class="footer-nav">
                                <li><a href="#">Sitemap</a>
                                </li>
                                <li><a href="#">Privacy Policy</a>
                                </li>
                                <li><a href="#">Contact</a>
                                </li>
                            </ul>
                        </div><!-- .col-md-6 -->
                    </div><!-- .row -->
                </div>
                <!-- End Copyright -->

            </div>
        </footer>
        <!-- End Footer Section -->
			</xpath>
			
		<xpath expr="//footer" position="after">
		 <!-- Go To Top Link -->
		    <a href="#" class="back-to-top"><i class="fa fa-angle-up"></i></a>
		    <div id="loader">
		        <div class="spinner">
		            <div class="dot1"></div>
		            <div class="dot2"></div>
		        </div>
		    </div>
		</xpath>
		
	</template>


		<!-- 404 page -->
		<template id="404_page_template" inherit_id="website.404" customize_show="True">
			<xpath expr="//div[@id='wrap']" position="replace">
				<div class="container">
					<div class="page-content">
						<div class="error-page">
							<h1>404</h1>
							<h3>File not Found</h3>
							<p>We're sorry, but the page you were looking for doesn't exist.</p>
							<div class="text-center"><a t-attf-href="/page/website.homepage" class="btn-system btn-small"> Back To Home</a></div>
							
						</div>
					</div>
				</div>
			</xpath>
		</template>	
		
    
	</data>
</openerp>