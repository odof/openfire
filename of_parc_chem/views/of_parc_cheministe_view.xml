        <!-- Parc installé form -->
    <record id="of_parc_installe_cheministe_form_view" model="ir.ui.view">
        <field name="name">of.parc.installe.cheministe.form.view</field>
        <field name="model">of.parc.installe</field>
        <field name="inherit_id" ref="of_parc_installe.of_parc_installe_form_view"/>
