<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_of_pack_operation_details_form" model="ir.ui.view">
        <field name="name">of.stock.pack.operation.details.form</field>
        <field name="model">stock.pack.operation</field>
        <field name="inherit_id" ref="stock.view_pack_operation_details_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_id']" position="after">
                <field name="move_name" groups="of_sale_stock.group_description_BL_variant"/>
            </xpath>
        </field>
    </record>

</odoo>
