<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="of_calculation_fuel_coef_view_form" model="ir.ui.view">
        <field name="name">of.calculation.fuel.coef.view.form</field>
        <field name="model">of.calculation.fuel.coef</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="coef"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="of_calculation_fuel_coef_view_tree" model="ir.ui.view">
        <field name="name">of.calculation.fuel.coef.view.tree</field>
        <field name="model">of.calculation.fuel.coef</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="coef"/>
            </tree>
        </field>
    </record>

</odoo>
